RoxygenNote: 7.1.2
LazyData: true
Imports:
  SummarizedExperiment,
  BiocSingular,
  ggplot2,
  cowplot,
  tidyverse
Depends: 
    R (>= 4.1.1)
Suggests:
  testthat
